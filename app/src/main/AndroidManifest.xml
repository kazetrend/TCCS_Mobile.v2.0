<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jirehcordova.tccs_mobilev2">

    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:excludeFromRecents="true"
        android:icon="@mipmap/ic_launcher"
        android:label="TCCS Mobile"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity
            android:name=".Launcher"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".PinForgot" />
        <activity android:name=".SetUpPinActivity" />
        <activity android:name=".ConfirmPin" />
        <activity android:name=".PinLockActivity" />
        <activity android:name=".LoginActivity" />

        <activity android:name=".MainActivity"/>

    </application>
</manifest>